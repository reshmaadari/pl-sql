
declare
grd char(1) := 'A';
begin
case grd
when 'A' then dbms_output.put_line(' grade is outstanding.');
when 'B' then dbms_output.put_line(' grade is excellent.');
when 'C' then dbms_output.put_line(' grade is very good.');
when 'D' then dbms_output.put_line(' grade is good.');
when 'F' then dbms_output.put_line(' grade is poor.');
else
dbms_output.put_line('No such grade is available.');
end case;
end;
